<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>dengluServlet</servlet-name>
    <servlet-class>service.dengluServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>dengluServlet</servlet-name>
    <url-pattern>/denglu</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>zhuceServlet</servlet-name>
    <servlet-class>service.zhuceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>zhuceServlet</servlet-name>
    <url-pattern>/zhuce</url-pattern>
  </servlet-mapping>

</web-app>
